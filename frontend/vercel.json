{
    "buildCommand": null,
    "outputDirectory": ".",
    "framework": null,
    "cleanUrls": false,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/downloads/(.*)",
            "headers": [
                {
                    "key": "Content-Disposition",
                    "value": "attachment"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0"
                }
            ]
        }
    ]
}