{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/downloads/(.*)",
            "dest": "public/downloads/$1"
        },
        {
            "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|zip))",
            "dest": "frontend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/index.html"
        }
    ],
    "regions": [
        "gru1"
    ]
}