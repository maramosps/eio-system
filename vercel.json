{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/engine/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/engine/ack.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/engine/schedule.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/engine/tasks.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/health",
            "dest": "/api/index.js?path=health"
        },
        {
            "src": "/api/engine/ack",
            "dest": "/api/engine/ack.js"
        },
        {
            "src": "/api/engine/schedule",
            "dest": "/api/engine/schedule.js"
        },
        {
            "src": "/api/engine/tasks",
            "dest": "/api/engine/tasks.js"
        },
        {
            "src": "/api/engine/(.*)",
            "dest": "/api/engine/index.js?path=$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js?path=$1"
        },
        {
            "src": "/dashboard",
            "dest": "/frontend/dashboard.html"
        },
        {
            "src": "/crm",
            "dest": "/frontend/crm.html"
        },
        {
            "src": "/login",
            "dest": "/frontend/login.html"
        },
        {
            "src": "/register",
            "dest": "/frontend/register.html"
        },
        {
            "src": "/termos",
            "dest": "/frontend/termos.html"
        },
        {
            "src": "/guia",
            "dest": "/frontend/guia.html"
        },
        {
            "src": "/quiz",
            "dest": "/frontend/quiz.html"
        },
        {
            "src": "/downloads/(.*)",
            "dest": "/frontend/downloads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}